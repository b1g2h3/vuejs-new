        "@types/webpack": "^4.41.21",
        "@vue/compiler-sfc": "^3.0.0-rc.9",
        "@vue/server-renderer": "^3.0.0-rc.5",
        "axios": "^0.20.0",
        "browser-sync": "^2.26.9",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "^7.0.2",
        "laravel-mix": "^5.0.4",
        "laravel-mix-purgecss": "^7.0.2",
        "laravel-mix-tailwind": "^12.0.1",
